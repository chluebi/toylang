

main() {
    a = [3, 5, 1, -1];
    
    i = 0;
    while (i < ?a) {
        j = 0;
        while (j < ?a) {
            if (a[j] > a[i]) {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            } else {}

            j = j + 1;
        }

        i = i + 1;
    }

    return a;
}